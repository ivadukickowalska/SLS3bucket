# Copy this to terraform.tfvars and fill in your values
# DO NOT commit terraform.tfvars to version control!

# Required variables
worker_pool_id         = "YOUR_WORKER_POOL_ID_HERE"
worker_pool_config     = "YOUR_WORKER_POOL_CONFIG_HERE"
worker_pool_private_key = "YOUR_WORKER_POOL_PRIVATE_KEY_HERE"
security_groups        = ["sg-xxxxxxxxx"]
vpc_subnets           = ["subnet-xxxxxxxx", "subnet-yyyyyyyy"]
spacelift_api_key_secret = "YOUR_API_KEY_SECRET_HERE"

# Optional overrides (these have defaults)
# spacelift_api_key_id = "YOUR_API_KEY_ID"
# spacelift_api_key_endpoint = "https://youraccount.app.spacelift.io"
# ec2_instance_type = "t3.micro"
# min_size = 0
# max_size = 10
# volume_size = 40
# volume_encryption = false
